/* pages/my/sheet/sheet.wxss */
.main {
  width: 100vw;
  height: 100vh;
  padding: 0 35rpx;
  background:rgba(248,248,248,1);
}
.main .row {
  width: 750rpx;
  margin-left: -35rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  /* padding: 30rpx 0; */
  font-size:28rpx;
font-family:Source Han Sans CN;
font-weight:500;
color:rgba(51,51,51,1);
padding: 30rpx 35rpx;
box-sizing: border-box;
border-bottom: 1rpx solid rgba(248,248,248,1);
}
.main .row:last-child{
  border-bottom:none;
}
.main .row .right{
  width: 15rpx;
  height: 28rpx;
}
.main .row .right image{
  width: 100%;
  height: 100%;
  margin-top: 3rpx;
}