.main {
  width: 750rpx;
  /* height: 1334rpx; */
  height: 100vh;
  background: rgba(248, 248, 248, 1);
}

.main .header {
  padding: 0 20rpx;
  background-color: #fff;
}

.title {
  background: rgba(248, 248, 248, 1);
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  padding-left: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.row {
  display: flex;
  background-color: #fff;
  padding: 0 40rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(238, 238, 238, 1);
}
.row:last-child{
  border-bottom: none;
}
.biaoqian {
  display: flex;
  align-items: center;
}

.biaoqian image {
  height: 29rpx;
  width: 16rpx;
  margin-left: 20rpx;
  /* margin-bottom: -30rpx; */
}
.wuren {
  padding-top: 20rpx;
  width: 100%;
  text-align: center;
}