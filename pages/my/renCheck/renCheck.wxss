.main{
  background:rgba(248,248,248,1);
  height: 100vh;
  width: 100vw;
}
.main .row{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 38rpx 34rpx;
  background-color: #fff;
  border-bottom: 1rpx solid rgba(216,216,216,1);
}
.main .row:last-child{
  border-bottom: none;
}
.main .row .left{
  display: flex;
}
.main .row .left .one{
 width: 110rpx;
 height: 110rpx;
 margin-right: 20rpx;
}
.main .row .left .one image{
  width: 100%;
  height: 100%;
  border-radius:50%;
}
.main .row .left .two{
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size:28rpx;
font-family:Source Han Sans CN;
font-weight:bold;
color:rgba(51,51,51,1);
}
.main .row .left .two .top{
  margin-top: 10rpx;
}
.main .row .left .two .bottom{
  font-size:28rpx;
font-family:Source Han Sans CN;
font-weight:500;
}
.main .row .left .two .bottom text{
  display: inline-block;
  color:rgba(255,189,85,1);
  margin-top: 6rpx;
}
.main .row .left .two .bottom1 text{
  display: inline-block;
  color:rgba(182,182,182,1);
  /* margin-top: 10rpx; */
  margin-bottom: -6rpx;
}
.main .row .left .two .bottom2 text{
  display: inline-block;
  color:rgba(177,206,250,1);
  margin-top: 6rpx;
}
.main .row .left .two .bottom image{
  display: inline-block;
  height: 39rpx;
  width: 39rpx;
  margin-right: 5px;
  margin-top: 20rpx;
  vertical-align: bottom;
  /* margin-top: -6rpx; */
}
.main .row .left .two .bottom1 image{
  display: inline-block;
  height: 31rpx;
  width: 31rpx;
  margin-right: 5px;
  margin-top: 26rpx;
  vertical-align: bottom;
  /* margin-top: -6rpx; */
}
.main .row .right{
  width: 14rpx;
  height: 25rpx;
}
.main .row .right image{
  width: 100%;
  height: 100%;
}
.wuren{
  width: 100%;
  text-align: center;
  padding-top: 100rpx;
}